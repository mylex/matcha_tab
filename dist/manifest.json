{
    "manifest_version": 2,
    "name": "MATCHA-EYE",
    "description": "Replace default new tab page in Chrome with a useful new tab page.",
    "version": "1.4.1",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "https://matcha-jp.com/",
        "storage",
        "topSites",
        "history",
        "chrome://favicon/",
        "unlimitedStorage"
    ],
    "content_security_policy": "script-src 'self' https://matcha-jp.com/ https://www.google-analytics.com; object-src 'self'",
    "icons": {
        "72": "img/touch-icon_72.png",
        "114": "img/touch-icon_114.png"
    }
}